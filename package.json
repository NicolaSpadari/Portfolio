{
	"name": "nuxt-template",
	"type": "module",
	"version": "1.0.0",
	"private": true,
	"packageManager": "pnpm@9.10.0",
	"description": "Personal portfolio",
	"author": "Nicola Spadari",
	"engines": {
		"node": ">=20"
	},
	"scripts": {
		"dev": "nuxt dev",
		"build": "nuxt build",
		"generate": "nuxt generate",
		"preview": "nuxt preview",
