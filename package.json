	"devDependencies": {
		"@antfu/eslint-config": "^3.10.0",
		"@hypernym/nuxt-gsap": "^2.4.3",
		"@nuxt/eslint": "^0.7.2",
		"@nuxt/icon": "^1.8.2",
		"@unocss/eslint-plugin": "^0.64.1",
		"@unocss/nuxt": "^0.64.1",
		"@vueuse/core": "^12.0.0",
		"@vueuse/nuxt": "^12.0.0",
		"eslint": "9.15.0",
		"nuxt": "^3.14.1592",
		"nuxt-gtag": "^3.0.2",
		"nuxt-security": "^2.1.4",
		"nuxt-svgo": "^4.0.9",
		"nuxt3-date-fns": "^0.2.0",
