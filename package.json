	"devDependencies": {
		"@antfu/eslint-config": "^3.6.0",
		"@hypernym/nuxt-gsap": "^2.4.3",
		"@nuxt/eslint": "^0.5.7",
		"@nuxt/icon": "^1.5.1",
		"@unocss/eslint-plugin": "^0.62.3",
		"@unocss/nuxt": "^0.62.3",
		"@vueuse/core": "^11.0.3",
		"@vueuse/nuxt": "^11.0.3",
		"eslint": "8.57.0",
		"nuxt": "^3.13.1",
		"nuxt-gtag": "^3.0.1",
		"nuxt-security": "2.0.0-beta.0",
		"nuxt-svgo": "^4.0.5",
